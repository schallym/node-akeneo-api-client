{
  "name": "akeneo-api-client",
  "version": "0.0.1",
  "description": "TypeScript client for Akeneo PIM API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint src/**/*.ts src/*.ts tests/**/*.ts tests/*.ts",
    "lint:check": "eslint src/**/*.ts src/*.ts tests/**/*.ts tests/*.ts && prettier --check src/**/*.ts tests/**/*.ts",
    "format": "prettier --write src/**/*.ts tests/**/*.ts"
  },
  "keywords": [
    "akeneo",
    "pim",
    "api",
    "client",
    "typescript",
    "ecommerce"
  ],
  "author": "Maximilien Schall-Fonteilles <maximilien.schall@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/schallym/akeneo-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/schallym/akeneo-api-client/issues"
  },
  "homepage": "https://github.com/schallym/akeneo-api-client#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^30.0.3",
    "nock": "^14.0.5",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
